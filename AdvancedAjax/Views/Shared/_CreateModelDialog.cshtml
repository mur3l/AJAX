<div id="DivCreateDialogHolder" class="model fade text-width:30%;" role="dialog">
    <div class="model-dialog">
        <div class="model-content">
            <div class="model-header">
                <h2>Create</h2>
                <button type="button" class="close" data-bs-dismiss="model">&times;></button>
            </div>

            <div id="DivCreateDialog" class="model-body"></div>

            <div class="model-footer">
                @*<button type="button" class="btn btn-danger" data-icon="Close" data-bs-dismiss="model">Close</button>*@

                <div class="btn-group">
                    <div>
                        <button id="dupBackBtn" type="button" class="btn btn-danger" data-icon="back" data-bs-dismiss="model">Back</button>
                    </div>
                    <div class="ml-4">
                        <input id="dupSubmitBtn" type="button" value="Save" class="btn btn-primary" onclick="submitModelForm()"/>
                    </div>
                </div>
                <button type="button" class="btn btn-danger" data-icon="Close" data-bs-dismiss="model">Close</button>
            </div>
        </div>
    </div>
</div>